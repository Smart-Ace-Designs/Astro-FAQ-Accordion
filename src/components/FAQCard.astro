---
import { getCollection } from "astro:content";
const faqs = await getCollection("faqs");
---

<section class="bg-theme-white rounded-lg p-4">
  <div class="flex flex-row space-x-5">
    <img src="/icon-star.svg" alt="Star" width="23" />
    <h1 class="text-theme-dark-purple text-[32px] font-extrabold">FAQs</h1>
  </div>
  <ul>{faqs.map(({ data }) => <li>{data.question}</li>)}</ul>
</section>
